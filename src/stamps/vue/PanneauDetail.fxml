<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<Scene xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stamps.controleur.PanneauDetail">
    <AnchorPane prefHeight="1000.0" prefWidth="1000.0">
      <children>
         <BorderPane prefHeight="1000.0" prefWidth="1000.0" styleClass="background" stylesheets="@/styleSheet.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <center>
               <ScrollPane fx:id="scrollPane">
                  <VBox fx:id="vbox" alignment="CENTER" prefHeight="-Infinity" prefWidth="-Infinity" BorderPane.alignment="CENTER">
                     <children>
                        <fx:include fx:id="PanneauInformation" source="PanneauInformation.fxml" />
                     </children>
                  </VBox>
               </ScrollPane>

            </center>
            <top>
               <HBox BorderPane.alignment="CENTER">
                  <children>
                     <Button mnemonicParsing="false" onAction="#changerGlobal" prefHeight="25.0" prefWidth="93.0" text="global" />
                     <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="123.0" text="sauvegarde" />
                     <Label fx:id="labelTitre" alignment="BASELINE_CENTER" contentDisplay="CENTER" prefHeight="24.0" prefWidth="823.0" text="Label" />
                  </children>
               </HBox>
            </top>
            <bottom>
               <Pane prefHeight="40.0" prefWidth="1000.0" BorderPane.alignment="CENTER">
                  <children>
                     <Button layoutX="14.0" layoutY="8.0" mnemonicParsing="false" onAction="#precedent" prefWidth="100.0" text="precedent" />
                     <Label fx:id="date" alignment="CENTER" layoutX="393.0" layoutY="1.0" prefHeight="38.0" prefWidth="214.0" text="Label" />
                     <Button layoutX="886.0" layoutY="8.0" mnemonicParsing="false" onAction="#suivant" prefHeight="25.0" prefWidth="100.0" text="suivant" />
                  </children>
               </Pane>
            </bottom>
            <left>
               <ImageView fx:id="image" fitHeight="500.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
                  <image>
                     <Image url="@/pasImage.jpeg" />
                  </image>
               </ImageView>
            </left>
            <right>
               <Button fx:id="ajoutInfo" mnemonicParsing="false" onAction="#ajouterInfo" text="ajouter info" BorderPane.alignment="CENTER" />
            </right>
         </BorderPane>
      </children></AnchorPane>
   <fill>
      <LinearGradient endX="1.0" endY="1.0">
         <stops>
            <Stop>
               <color>
                  <Color />
               </color>
            </Stop>
            <Stop offset="1.0">
               <color>
                  <Color red="1.0" green="1.0" blue="1.0" />
               </color>
            </Stop>
         </stops>
      </LinearGradient>
   </fill>
</Scene>
