<Scene xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stamps.controleur.PanneauDetail">
    <AnchorPane prefHeight="1000.0" prefWidth="1000.0">
      <children>
         <BorderPane prefHeight="1000.0" prefWidth="1000.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <bottom>
               <ButtonBar prefHeight="40.0" prefWidth="1000.0" BorderPane.alignment="CENTER">
                 <buttons>
                     <Button alignment="CENTER" minWidth="0.0" mnemonicParsing="false" onAction="#precedant" prefHeight="25.0" prefWidth="100.0" text="precedant">
                        <cursor>
                           <Cursor fx:constant="OPEN_HAND" />
                        </cursor></Button>
                     <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" text="date" />
                   <Button alignment="CENTER" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#suivant" prefHeight="25.0" prefWidth="100.0" text="suivant">
                        <cursor>
                           <Cursor fx:constant="CLOSED_HAND" />
                        </cursor></Button>
                 </buttons>
               </ButtonBar>
            </bottom>
            <center>
               <HBox fx:id="hbox" alignment="CENTER" prefHeight="800.0" prefWidth="1000.0" BorderPane.alignment="CENTER">
                  <children>
                     <ImageView fitHeight="500.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true" />
                     <VBox alignment="CENTER_RIGHT" prefHeight="800.0" prefWidth="500.0" />
                  </children>
               </HBox>
            </center>
            <top>
               <HBox BorderPane.alignment="CENTER">
                  <children>
                     <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="93.0" text="global" />
                     <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="123.0" text="sauvegarde" />
                     <Label fx:id="labelTitre" alignment="CENTER" prefHeight="24.0" prefWidth="823.0" text="Label" />
                  </children>
               </HBox>
            </top>
         </BorderPane>
      </children></AnchorPane>
   <fill>
      <LinearGradient endX="1.0" endY="1.0">
         <stops>
            <Stop>
               <color>
                  <Color />
               </color>
            </Stop>
            <Stop offset="1.0">
               <color>
                  <Color red="1.0" green="1.0" blue="1.0" />
               </color>
            </Stop>
         </stops>
      </LinearGradient>
   </fill>
</Scene>
