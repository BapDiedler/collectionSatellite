<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<Scene xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stamps.controleur.PanneauGlobal">
    <AnchorPane prefHeight="1000.0">
        <children>
                <BorderPane layoutX="-2.0" layoutY="-2.0" prefHeight="1000.0" prefWidth="1000.0" styleClass="background" stylesheets="@/styleSheet.css">
                <top>
                   <fx:include fx:id="PanneauMenu" source="PanneauMenu.fxml" />
                </top>
                    <center>
                        <fx:include fx:id="PanneauCentre" prefHeight="300.0" prefWidth="600.0" source="PanneauCentral.fxml" BorderPane.alignment="CENTER" />
                    </center>
                <bottom>
                    <fx:include fx:id="PanneauOutils" source="PanneauOutils.fxml" />
                </bottom>
            </BorderPane>
        </children></AnchorPane>
</Scene>
